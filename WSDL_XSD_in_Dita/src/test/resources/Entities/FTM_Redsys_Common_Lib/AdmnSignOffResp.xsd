      <xs:documentation xml:lang="EN" source="Definition">Specifies a character string with a maximum length of 4 characters and allowed characteres.</xs:documentation> 
    </xs:annotation> 
    <xs:restriction base="xs:string"> 
      <xs:minLength value="1"/> 
      <xs:maxLength value="4"/> 
      <xs:pattern value="[ACCP|RJCT]{4,4}"></xs:pattern>
    </xs:restriction> 
  </xs:simpleType>
   
